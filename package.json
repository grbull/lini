{
  "name": "lini",
  "version": "2.13.2",
  "description": "A progressive web application for tracking Tv Shows.",
  "main": "index.js",
  "author": "graham.bull@gmail.com",
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/grbull/lini.git"
  },
  "scripts": {
    "start": "node dist/server/main.js",
    "dev": "concurrently --kill-others 'npm run server:dev' 'npm run client:dev'",
    "build": "npm run server:build && npm run client:build",
    "client:dev": "webpack serve --inline --hot",
    "client:build": "rimraf dist/client/ && webpack --mode=production && chmod 755 -R dist/client/",
    "server:dev": "rimraf dist/server/ && nest start --watch",
    "server:build": "rimraf dist/server/ && nest build",
    "db:sync": "npm run server:build && typeorm schema:sync",
    "db:drop": "npm run server:build && typeorm schema:drop",
    "db:migration:generate": "npm run server:build && typeorm migration:generate -n ",
    "db:migration:run": "npm run server:build && typeorm migration:run",
    "util:seed": "npm run server:build && node --max-old-space-size=250 dist/server/scripts/seed.js",
    "util:updateTest": "npm run server:build && node --max-old-space-size=250 dist/server/scripts/update.js",
    "util:pushTest": "npm run server:build && node --max-old-space-size=250 dist/server/scripts/test_push.js",
    "lint": "eslint 'src/**/*.{tsx,ts}' && prettier --check 'src/**/*.(ts|tsx)'",
    "prepare": "husky install",
    "release": "standard-version",
    "test": "TZ=GMT jest"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/common": "^7.6.17",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.17",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.17",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/throttler": "^1.1.4",
    "@nestjs/typeorm": "^7.1.5",
    "@reduxjs/toolkit": "^1.6.0",
    "autoprefixer": "^10.2.6",
    "axios": "^0.21.1",
    "axios-rate-limit": "^1.3.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "classnames": "^2.3.1",
    "connect-redis": "^6.0.0",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.6",
    "dotenv": "^10.0.0",
    "dotenv-webpack": "^7.0.3",
    "express-session": "^1.17.2",
    "helmet": "^4.6.0",
    "html-webpack-plugin": "^5.3.1",
    "ioredis": "^4.27.5",
    "luxon": "^1.27.0",
    "mini-css-extract-plugin": "^1.6.0",
    "nodemailer": "^6.6.1",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.1",
    "pg": "^8.6.0",
    "postcss": "^8.3.2",
    "postcss-loader": "^6.1.0",
    "pug": "^3.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-media-hook": "^0.4.9",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux-persist": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.7",
    "standard-version": "^9.3.0",
    "tailwindcss": "^2.1.4",
    "terser-webpack-plugin": "^5.1.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "typeorm": "^0.2.34",
    "typescript": "^4.3.2",
    "web-push": "^3.4.4",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "winston": "^3.3.3",
    "workbox-webpack-plugin": "^6.1.5"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^12.1.4",
    "@golevelup/ts-jest": "^0.3.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/connect-redis": "^0.0.16",
    "@types/cron": "^1.7.2",
    "@types/csurf": "^1.11.1",
    "@types/express": "^4.17.12",
    "@types/express-session": "^1.17.3",
    "@types/ioredis": "^4.26.4",
